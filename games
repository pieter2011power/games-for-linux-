import webbrowser
import time
print('welcome')
print('press 1 to set recommend settings for u pc  pres 2 to not set recommend settings')
qw2 = input('')
if qw2 == ("1"):
  print ('setting the settings')
  time.sleep(2)
if qw2 == ("2"):
  time.sleep(2)
  print('opens settings')
  time.sleep(2)
  print('type high for high textures type medium for medium textures type low for low textures')
  texures = input('\n')

  print('seleced: %s' % texures) 


print('open the games in 5 seconds  (YOU NEED CHROME AS STANDARD BROWSER)')
time.sleep(5)
webbrowser.open('https://play.geforcenow.com/mall/#/layout/games')
